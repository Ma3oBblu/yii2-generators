{
    "name": "ma3obblu/yii2-generators",
    "description": "Generators for Yii2(gii)",
    "type": "yii2-extension",
    "keywords": ["yii2", "yii 2", "gii"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ruslan Gilyazetdinov",
            "email": "ma3obblu@gmail.com",
            "homepage": "https://vk.com/ma3obblu"
        }
    ],
    "support": {
        "issues": "https://github.com/Ma3oBblu/yii2-generators/issues?state=open",
        "source": "https://github.com/Ma3oBblu/yii2-generators"
    },
    "require": {
        "yiisoft/yii2-gii": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "ma3obblu\\gii\\generators\\": "src/",
            "ma3obblu\\gii\\generators\\form\\": "src/form/",
            "ma3obblu\\gii\\generators\\entity\\": "src/entity/",
            "ma3obblu\\gii\\generators\\handler\\": "src/handler/",
            "ma3obblu\\gii\\generators\\crud\\": "src/crud/",
            "ma3obblu\\gii\\generators\\fixture\\": "src/fixture/",
            "ma3obblu\\gii\\generators\\tests\\": "tests/",
            "ma3obblu\\gii\\generators\\tests\\form\\": "tests/form/",
            "ma3obblu\\gii\\generators\\tests\\entity\\": "tests/entity/",
            "ma3obblu\\gii\\generators\\tests\\handler\\": "tests/handler/",
            "ma3obblu\\gii\\generators\\tests\\crud\\": "tests/crud/",
            "ma3obblu\\gii\\generators\\tests\\fixture\\": "tests/fixture/",
            "ma3obblu\\gii\\generators\\helpers\\": "src/helpers/"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}